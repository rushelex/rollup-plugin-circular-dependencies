{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": [
      "node",
      "vitest/globals",
      "./src/reset.d.ts"
    ],
    "allowImportingTsExtensions": true,
    "allowJs": true,
    "declaration": false,
    "declarationDir": null,
    "emitDeclarationOnly": false,
    "noEmit": true,
    "outDir": "dist"
  },
  "include": ["src", "*.ts", "*.d.ts"]
}
